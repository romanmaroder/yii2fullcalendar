{
	"name": "romanmaroder/yii2fullcalendar",
	"description": "Yii2 fullcalendar Widgets",
	"keywords": ["yii2", "fullcalendar","widget"],
	"type": "yii2-extension",
	"license": "MIT",
	"support": {
		"source": "https://github.com/philippfrenzel/yii2fullcalendar"
	},
	"minimum-stability": "stable",
	"require": {
		"bower-asset/fullcalendar": "6.1.9",
		"bower-asset/fullcalendar-scheduler" : "6.1.9"
	},
	"require-dev": {
		"codeception/codeception": "*"
	},
	"config": {
	    "process-timeout": 1800,
	      "fxp-asset": {
		"repositories": {
		  "bower-asset/eve": {
		  	"type": "bower-vcs",
		  	"url": "https://github.com/adobe-webplatform/eve.git"
		  }
		}
	    }
	},
	"conflict": {
		"bower-asset/fullcalendar": "^4.3.0"
	},
	"autoload": {
		"psr-4": { "yii2fullcalendar\\": "" }
	}
}
